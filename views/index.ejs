        .glyphicon-thumbs-up {
            cursor: pointer
        }
  </style>
</head>

<body ng-app="amacApp">
    <div class="navigaton" ng-controller="NavCtrl">
    <nav class="navbar navbar-default pull-right" >
        <ul class="nav navbar-nav">
            <li ng-show="isLoggedIn()"><a>{{ currentUser() }}</a></li>
            <li ng-show="isLoggedIn()"><a>{{ currentUserType() }}</a></li>

            <li ng-show="isLoggedIn()"><a href="#" ng-click="logOut()">Log Out</a></li>
            <li ng-hide="isLoggedIn()"><a href="/#/login">Log In</a></li>
            <li ng-hide="isLoggedIn()"><a href="/#/register">Register</a></li>
        </ul>
    </nav>
    <div class="row logo">
        <img style="width:100; height:100;" src="images/logo_Spia.png" />
    </div>
    <div class="col-md-6 col-md-offset-3" style="margin-top:69px;">
        <ui-view></ui-view>
    </div>
  </div>

    <script type="text/ng-template" id="/home.html">
      <ul class="nav nav-pills nav-justified">
          <li class="navPill" ng-show="{{currentUserType() == 'planeacion' || currentUserType == 'almacen' || currentUserType() == 'productoTer'}}"><a  href="/#/products">Catalogo</a></li>
          <li class="navPill active"><a href="/#/home">Ordenes</a></li>
          <li class="navPill" ng-show="{{currentUserType() == 'ventas' || currentUserType() == 'productoTer' || currentUserType() == 'planeacion' || currentUserType() == 'prensa' || currentUserType() == 'almacen' || currentUserType() == 'acabados' || currentUserType() == 'calidad' || currentUserType() == 'entregas' || currentUserType() == 'almacen'}}"><a href="/#/endproducts">Producto Terminado</a></li>
          <li class="navPill"><a href="/#/register">Agregar Usuario</a></li>
      </ul>
        <table class="table table-hover">

            <thead>
                <tr>
                    <th>Orden</th>
                    <th>Estatus</th>
                    <th></th>
                </tr>
            </thead>
            <tbody ng-repeat="post in posts | orderBy: '-upvotes'">
                <tr>
                    <td><span ng-hide="post.link">{{post.title}}</span><span ng-show="post.author">posted by <a>{{post.author}}</a></span></td>
                    <td>
                        <span ng-show="post.progress == 0"><b>No a Iniciado</b></span>
                        <span ng-show="post.progress == 10"><b>Diseño</b></span>
                        <span ng-show="post.progress == 20"><b>Preprensa</b></span>
                        <span ng-show="post.progress == 30"><b>planeacion</b></span>
                        <span ng-show="post.progress == 50"><b>Almacen</b></span>
                        <span ng-show="post.progress == 60"><b>Prensa</b></span>
                        <span ng-show="post.progress == 65"><b>Acabados</b></span>
                        <span ng-show="post.progress == 70"><b>Producto Terminado</b></span>
                        <span ng-show="post.progress == 80"><b>Calidad</b></span>
                        <span ng-show="post.progress == 90"><b>Entregas</b></span>
                        <span ng-show="post.progress == 100"><b>Orden Completada</b></span>
                    </td>
                    <td><span><a href="#/posts/{{post._id}}">ver orden</a>({{post.comments.length}})</span></td>
                    <td><div ng-show="BlinkerActivator(post.dateSalida)" class="fa fa-circle text-danger Blink"></div></td>
                </tr>
            </tbody>
        </table>

        <form ng-submit="addPost()" ng-show="isLoggedIn() && currentUserType() == 'ventas' || currentUserType == 'planeacion'" style="margin-top:30px">
            <h3>Agrega una Orden:</h3>

            <div class="form-group">
                <input type="text" class="form-control" placeholder="Titulo" ng-model="title">
            </div>

            <div class="form-group">
                <input type="text" class="form-control" placeholder="Nombre de Cliente" ng-model="nombreCliente">
            </div>

            <div class="row">
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class "form-group">
                        <input type="text" class="form-control" placeholder="Entrada" autocomplete="off" id="datepicker" readonly  ng-model="dateEntrada">
                    </div>
                </div>

                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class "form-group">
                        <input type="text" class="form-control" placeholder="Impresión" autocomplete="off" id="datepicker2" readonly ng-model="dateImpresion">
                    </div>
                </div>

                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class "form-group">
                        <input type="text" class="form-control" placeholder="Acabado" autocomplete="off" id="datepicker3" readonly  ng-model="dateAcabado">
                    </div>
                </div>

                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class "form-group">
                        <input type="text" class="form-control" placeholder="Salida" autocomplete="off" id="datepicker4" readonly ng-model="dateSalida">
                    </div>
                </div>
            </div>
            <br></br>
            <button type="submit" class="btn btn-primary">Post</button>
        </form>

        <div ng-hide="isLoggedIn()">
            <h3>Ocupas <a href="/#/login">Ingresar</a> o <a href="/#/register">Registrarte</a> antes de poner una orden</h3>
        </div>

    </script>

    <script type="text/ng-template" id="/posts.html">
      <ul class="nav nav-pills nav-justified">
