    <script type="text/ng-template" id="/products.html">
      <ul class="nav nav-pills nav-justified">
          <li class="navPill" ng-show="{{currentUserType() == 'planeacion' || currentUserType == 'almacen' || currentUserType() == 'productoTer'}}"><a  href="/#/products">Catalogo</a></li>
          <li class="navPill active"><a href="/#/home">Ordenes</a></li>
          <li class="navPill" ng-show="{{currentUserType() == 'ventas' || currentUserType() == 'productoTer' || currentUserType() == 'planeacion' || currentUserType() == 'prensa' || currentUserType() == 'almacen' || currentUserType() == 'acabados' || currentUserType() == 'calidad' || currentUserType() == 'entregas' || currentUserType() == 'almacen'}}"><a href="/#/endproducts">Producto Terminado</a></li>
          <li class="navPill"><a href="/#/register">Agregar Usuario</a></li>
      </ul>
        <div class="form-group" style="padding-top:50px;">
          <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
              <input type="text" placeholder="Buscar: " ng-model="filter" class="form-control">
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <select ng-model="filter" class="form-control">
                  <option value="Papel">Papel</option>
                  <option value="Tintas">Tintas</option>
                  <option value="Quimicos">Quimicos</option>
                  <option value="Miscelanea">Miscelanea</option>
                  <option value="Acetatos">Acetatos</option>
                  <option value="Carton">Carton</option>
                  <option value="Placas">Placas</option>
                </select>
            </div>
          </div>
      </div>


        <div class="form-group">
          <button type="button" class="btn btn-primary" data-toggle="modal" data-target=".bs-example-modal-lg">Agrega Producto</button>
        </div>

        <div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                  <div class="modelContent">
                    <form ng-submit="addProduct()" ng-show="isLoggedIn()" style="margin-top:30px">
                        <h3 style="text-align:center;">Agrega un Producto</h3>
                        <div class="form-group">
                            <h3>Informacion Basica</h4>
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                        <input type="text" class="form-control" placeholder="Title" ng-model="title">
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                        <input type="text" class="form-control" placeholder="Descripcion" ng-model="description">
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                        <input type="text" class="form-control" placeholder="tina, azul, describeme..." ng-model="tags">
                                    </div>
                                </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-6">
                                    <input type="text" class="form-control" placeholder="Provedor" ng-model="suppplier">
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-6">
                                    <input type="number" class="form-control" placeholder="Cantidad" ng-model="quantity">
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12">
                                    <div class="form-group">
                                      <select ng-model="unit" class="form-control">
                                        <option value="piezas">Piezas</option>
                                        <option value="libras">Lbs</option>
                                        <option value="galones">Galones</option>
                                      </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
